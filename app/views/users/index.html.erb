<%= render 'books/list', book: @book %>

 <h1>Books</h1>
<% @users.each do |user| %>
      <table>
        <thead>
          <tr>
            <th>image</th>
            <th>name</th>
            <th colspan="3"></th>
          </tr>
      </thead>

          <%= image_tag user_path(user.id) do %>
            <%= image_tag @users.get_image %>
          <% end %>
      <tbody>
        <tr>
          <td><%= image_tag @user.get_profile_image(100,100) %></td>
          <td><%= user.name %></td>
          <td><%= link_to "Show", user_path(@user.id) %></td>
        </tr>
      </tbody>

      </table>
  </div>
<% end %>